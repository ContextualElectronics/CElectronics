#include <stdio.h>
#include <stdlib.h>


void main(void)
{
    uint8_t TempData;

    Led_Init();
    Tick_init();

    SerialPort2.Open(9600);

    SerialPort2.SendByte(0x0C); // clear terminal

    SerialPort2.SendString((int8_t *)"Hello World\r\n");

    for ( ;; )
    {
        if(SerialPort2.GetByte(&TempData))
        {
            SerialPort2.SendByte(TempData);
        }
    }
}
